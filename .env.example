# Supabase Public Keys (Safe to expose)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxxxx

# Database Connection (Development only)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-password
POSTGRES_HOST=db.your-project.supabase.co
POSTGRES_DATABASE=postgres

# Supabase Secret Keys (Never commit to repo)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=sb_service_role_xxxxx
SUPABASE_SECRET_KEY=sb_secret_xxxxx
SUPABASE_JWT_SECRET=your-jwt-secret

# Database URLs
POSTGRES_URL=postgresql://postgres.xxxxx:password@host:6543/postgres?sslmode=require&supa=base-pooler.x
POSTGRES_URL_NON_POOLING=postgresql://postgres.xxxxx:password@host:5432/postgres?sslmode=require
POSTGRES_PRISMA_URL=postgresql://postgres.xxxxx:password@host:6543/postgres?sslmode=require&pgbouncer=true
