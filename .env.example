# Supabase Public Keys (Safe to expose)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=sb_publishable_xxxxx

# Database Connection (Development only)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-password
POSTGRES_HOST=db.your-project.supabase.co
POSTGRES_DATABASE=postgres

# Supabase Secret Keys (Never commit to repo)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=sb_service_role_xxxxx
SUPABASE_SECRET_KEY=sb_secret_xxxxx
SUPABASE_JWT_SECRET=your-jwt-secret

# Database URLs
POSTGRES_URL=postgresql://postgres.xxxxx:password@host:6543/postgres?sslmode=require&supa=base-pooler.x
POSTGRES_URL_NON_POOLING=postgresql://postgres.xxxxx:password@host:5432/postgres?sslmode=require
POSTGRES_PRISMA_URL=postgresql://postgres.xxxxx:password@host:6543/postgres?sslmode=require&pgbouncer=true

# Stripe Configuration (Add your keys from https://dashboard.stripe.com/apikeys)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here
STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Brevo Configuration (Add your keys from https://www.brevo.com)
BREVO_API_KEY=your_brevo_api_key_here
BREVO_SMTP_KEY=your_brevo_smtp_key_here
BREVO_SMTP_USER=your_brevo_email@example.com

# Custom Authentication (JWT + Sessions)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_JWT_SECRET=your_secret_key_here_minimum_32_chars

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_DEV_SUPABASE_REDIRECT_URL=http://localhost:3000/auth/callback
